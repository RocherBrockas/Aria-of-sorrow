{
    "map_enabled": true,
    "auto_pin_capturing_locations": false,
    "auto_unpin_on_clear": false,
    "layout_swap_left_right": false,
    "disabled_image_filter": "grayscale, dim",
    "layouts": {
        "shared_item_grid": {
            "type": "itemgrid",
            "h_alignment": "center",
            "item_margin": "1,2",
            "rows": [
                [ "backdash", "djump", "hyppogriph", "bookblue", "bookred", "bookyellow" ],
                [ "blackp", "slide", "divekick", "creaking", "manticore", "hammer" ],
                [ "flyingarmor", "skula", "undine", "golem", "death", "legion" ],
                [ "rush", "galamoth", "bat", "balore", "headhunter", "graham" ]
            ]
        },
        "tracker_capture_item": {
            "type": "container",
            "content": {
                "type": "itemgrid",
                "h_alignment": "center",
                "item_margin": "1,2",
                "rows": [
                    [ "backdash", "djump", "hyppogriph", "bookblue", "bookred", "bookyellow" ],
                    [ "blackp", "slide", "divekick", "creaking", "manticore", "hammer" ],
                    [ "flyingarmor", "skula", "undine", "golem", "death", "legion" ],
                    [ "rush", "galamoth", "bat", "balore", "headhunter", "graham" ]
                ]
            }
        },
        "tracker_default": {
            "type": "container",
            "background": "#004080",
            "content": {
                "type": "dock",
                "dropshadow": false,
                "content": [
                    {
                        "type": "dock",
                        "dock": "left",
                        "v_alignment": "stretch",
                        "content": [
                            {
                                "type": "group",
                                "header": "Items",
                                "dock": "top",
                                "content": {
                                    "type": "layout",
                                    "key": "shared_item_grid"
                                }
                            },
                            {
                                "type": "group",
                                "header": "Pinned Locations",
                                "content": {
                                    "type": "recentpins",
                                    "style": "wrap",
                                    "h_alignment": "stretch",
                                    "v_alignment": "stretch",
                                    "orientation": "vertical",
                                    "compact": true
                                }
                            }
                        ]
                    },
                    {
                        "type": "map"
                    }
                ]
            }
        },
        "tracker_horizontal": {
            "type": "container",
            "background": "#004080",
            "content": {
                "type": "dock",
                "dropshadow": false,
                "content": [
                    {
                        "type": "dock",
                        "dock": "bottom",
                        "content": [
                            {
                                "type": "group",
                                "header": "Items",
                                "dock": "left",
                                "margin": "0,0,3,0",
                                "content": {
                                    "type": "layout",
                                    "h_alignment": "center",
                                    "v_alignment": "center",
                                    "key": "shared_item_grid"
                                }
                            },
                            {
                                "type": "group",
                                "header": "Pinned Locations",
                                "content": {
                                    "type": "recentpins",
                                    "style": "wrap",
                                    "h_alignment": "stretch",
                                    "v_alignment": "stretch",
                                    "orientation": "horizontal",
                                    "compact": true
                                }
                            }
                        ]
                    },
                    {
                        "type": "map"
                    }
                ]
            }
        }
    }
}
